{
  "name": "ur-validator",
  "version": "0.0.1-1",
  "description": "JavaScript and TypeScript validation library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "scripts": {
    "dev": "tsc -p . -w",
    "build": "tsc -p .",
    "test": "jest ./tests",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src ./tests",
    "lint:fix": "eslint ./src ./tests --fix",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags",
    "prerelease": "npm version prerelease && git push --follow-tags"
  },
  "pre-commit": [
    "lint:fix"
  ],
  "files": [
    "lib"
  ],
  "keywords": [
    "validation",
    "validator",
    "javascript",
    "typescript"
  ],
  "author": "Garik Freedomyan <garikfreedomyan@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ur-apps/validator.git"
  },
  "homepage": "https://github.com/ur-apps/validator#readme",
  "bugs": {
    "url": "https://github.com/ur-apps/validator/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
