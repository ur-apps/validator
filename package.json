{
  "name": "ur-validator",
  "version": "0.0.1-2",
  "description": "JavaScript and TypeScript validation library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "tsc -p . -w",
    "build": "tsc -p .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "release:patch": "npm version patch && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:major": "npm version major && git push --follow-tags",
    "prerelease": "npm version prerelease && git push --follow-tags"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "validation",
    "validator",
    "javascript",
    "typescript"
  ],
  "author": "Garik Freedomyan <garikfreedomyan@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ur-apps/validator.git"
  },
  "homepage": "https://github.com/ur-apps/validator#readme",
  "bugs": {
    "url": "https://github.com/ur-apps/validator/issues"
  },
  "dependencies": {
    "@ur-apps/common": "^0.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/jest": "^30.0.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.38.0"
  }
}
